<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hakanmehmed.trainapp.androidtrainapp.LocationFragment"
    android:orientation="vertical">

    <fragment xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map_fragment"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.hakanmehmed.trainapp.androidtrainapp.LocationFragment"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <com.hakanmehmed.trainapp.androidtrainapp.InstantAutoComplete
        android:id="@+id/searchRoutes"
        android:inputType="text"
        android:drawableEnd="@drawable/ic_close_48px"
        android:hint="@string/see_subscribed_routes"
        android:background="#95ffffff"
        android:textColor="@android:color/black"
        android:text=""
        android:ems="10"
        android:layout_width="320dp"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:padding="12dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="14dp"
        android:layout_alignParentTop="true"
        />

</RelativeLayout>
